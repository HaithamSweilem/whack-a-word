{
   "manifest_version": 3,
   "name": "Whack a Word",
   "description": "Set a word and whack it to see interesting effects!",
   "version": "1.3",
   "action": {
      "default_icon": {
         "128": "/images/gavel-solid-128.png",
         "16": "/images/gavel-solid-16.png",
         "32": "/images/gavel-solid-32.png",
         "48": "/images/gavel-solid-48.png"
      },
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "/images/gavel-solid-128.png",
      "16": "/images/gavel-solid-16.png",
      "32": "/images/gavel-solid-32.png",
      "48": "/images/gavel-solid-48.png"
   },
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx"
}
